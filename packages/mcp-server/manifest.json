{
  "dxt_version": "0.1",
  "name": "api-dental-mcp",
  "version": "1.3.0",
  "description": "The official MCP Server for the API Dental Pro API",
  "author": {
    "name": "API Dental Pro",
    "email": "info@api.dental"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/api-dental/api-dental-typescript-sdk.git"
  },
  "homepage": "https://github.com/api-dental/api-dental-typescript-sdk/tree/main/packages/mcp-server#readme",
  "documentation": "https://api.dental/docs",
  "server": {
    "type": "node",
    "entry_point": "${__dirname}/index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "API_DENTAL_API_KEY": "${user_config.API_DENTAL_API_KEY}",
        "API_DENTAL_PRO_SDK_SOURCE": "${user_config.API_DENTAL_PRO_SDK_SOURCE}",
        "API_DENTAL_PRO_SDK_LANG": "${user_config.API_DENTAL_PRO_SDK_LANG}"
      }
    }
  },
  "user_config": {
    "API_DENTAL_API_KEY": {
      "title": "api_key",
      "description": "",
      "required": true,
      "type": "string"
    },
    "API_DENTAL_PRO_SDK_SOURCE": {
      "title": "sdk_source",
      "description": "",
      "required": false,
      "type": "string"
    },
    "API_DENTAL_PRO_SDK_LANG": {
      "title": "sdk_lang",
      "description": "",
      "required": false,
      "type": "string"
    }
  },
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
